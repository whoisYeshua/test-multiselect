{
  "name": "@ui/design-system",
  "version": "8.14.4",
  "description": "ui-design-system",
  "files": [
    "accessories/*",
    "controls/*",
    "grid/*",
    "helpers/*",
    "pages/*",
    "wrappers/*",
    "icons/*",
    "styles/",
    "types/"
  ],
  "scripts": {
    "analyze": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.analyze.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.js --progress",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.prod.js --progress",
    "prepublishOnly": "npm run build",
    "publish:beta": "npm publish --tag beta",
    "start": "webpack serve --config ./webpack/webpack.dev.js",
    "test": "jest",
    "coverage:diffs": "npx jest --coverage --changedSince=develop",
    "coverage:watch": "npx jest --coverage --changedSince=develop --watch",
    "coverage:all": "npx jest --coverage",
    "lint": "npx eslint . --ext .js,.ts,.tsx",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier --list-different \"src/**/*.{js,ts,tsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-icons": "rm -rf ./src/icons && mkdir -p ./src/icons && node ./src/icons_helpers/build-icons.js && npm run prettier:fix",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run coverage:all"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@git.mos.renins.com:iris/front/ui-design-system.git"
  },
  "author": "Front Team",
  "license": "ISC",
